  "description": "A React Express Mongo Boilerplate to get started to development.",
  "main": "index.jsx",
  "scripts": {
    "watch-client": "cross-env NODE_ENV=development node server/index.js",
    "start": "cross-env NODE_ENV=production webpack --config webpack.prod.js & cross-env NODE_ENV=production node server/api/bin/index.js",
    "start-dev-prod": "cross-env NODE_ENV=development PORT=7788 webpack --config webpack.prod.js & cross-env NODE_ENV=development PORT=7788 node server/api/bin/index.js",
    "start-dev": "cross-env NODE_ENV=development PORT=7788 node server/index.js",
    "start-dev-api": "cross-env NODE_ENV=development PORT=7788 nodemon --watch server/api server/api/bin/index.js",
